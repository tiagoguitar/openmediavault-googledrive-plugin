#!/usr/bin/make -f

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE = 1

%: 
	dh $@

override_dh_auto_install:
	mkdir -p $(CURDIR)/debian/openmediavault-googledrive/usr/share/openmediavault/engined/rpc
	mkdir -p $(CURDIR)/debian/openmediavault-googledrive/usr/share/openmediavault/workbench/navigation
	mkdir -p $(CURDIR)/debian/openmediavault-googledrive/usr/share/openmediavault/workbench/dashboard
	mkdir -p $(CURDIR)/debian/openmediavault-googledrive/usr/share/openmediavault/workbench/component
	mkdir -p $(CURDIR)/debian/openmediavault-googledrive/usr/share/openmediavault/workbench/route
	mkdir -p $(CURDIR)/debian/openmediavault-googledrive/etc/openmediavault/conf.d

override_dh_install:
	dh_install

override_dh_clean:
	dh_clean

